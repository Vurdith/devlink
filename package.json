{
  "name": "devlink",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3000",
    "build": "prisma generate && next build --turbopack",
    "start": "next start",
    "test": "vitest run",
    "lint": "eslint",
    "public": "npx cloudflared tunnel --url http://localhost:3000",
    "public:dev": "concurrently \"npm run dev\" \"npx cloudflared tunnel --url http://localhost:3000\"",
    "tunnel": "npx cloudflared tunnel --url http://localhost:3000",
    "serveo": "ssh -R 80:localhost:3000 serveo.net",
    "public:serveo": "concurrently \"npm run dev\" \"ssh -R 80:localhost:3000 serveo.net\"",
    "cloudflare": "npx cloudflared tunnel --url http://localhost:3000",
    "public:cf": "concurrently \"npm run dev\" \"npx cloudflared tunnel --url http://localhost:3000\"",
    "kill": "taskkill /f /im node.exe & taskkill /f /im cloudflared.exe",
    "stop": "taskkill /f /im node.exe & taskkill /f /im cloudflared.exe"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.15.0",
    "@sentry/nextjs": "^10.27.0",
    "@supabase/supabase-js": "^2.86.0",
    "@types/nodemailer": "^7.0.2",
    "@types/recharts": "^1.8.29",
    "@upstash/redis": "^1.34.3",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "emoji-picker-react": "^4.13.3",
    "framer-motion": "^12.23.12",
    "ioredis": "^5.4.2",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "openai": "^5.21.0",
    "prisma": "^6.15.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.4.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^2.1.9"
  }
}
