{
  "name": "devlink",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "prisma generate",
    "dev": "next dev --turbopack --port 3000",
    "build": "prisma generate && next build --turbopack",
    "postinstall": "prisma generate",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:load": "k6 run loadtests/smoke.js",
    "test:load:messaging": "k6 run loadtests/messaging.js",
    "lint": "eslint",
    "tunnel": "npx cloudflared tunnel --url http://localhost:3000",
    "tunnel:dev": "concurrently \"npm run dev\" \"npm run tunnel\"",
    "kill": "taskkill /f /im node.exe & taskkill /f /im cloudflared.exe"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.990.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.69.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.212.0",
    "@opentelemetry/sdk-node": "^0.212.0",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@sentry/nextjs": "^10.27.0",
    "@supabase/supabase-js": "^2.86.0",
    "@upstash/redis": "^1.34.3",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "emoji-picker-react": "^4.13.3",
    "file-type": "^21.3.0",
    "framer-motion": "^12.23.12",
    "ioredis": "^5.4.2",
    "next": "^16.0.7",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.7",
    "pg": "^8.16.3",
    "prisma": "^7.1.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "recharts": "^3.4.1",
    "tailwind-merge": "^3.3.1"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed-skills.ts"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24.10.1",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "baseline-browser-mapping": "^2.9.19",
    "concurrently": "^9.1.0",
    "eslint": "^9",
    "eslint-config-next": "^16.0.7",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.15"
  }
}
