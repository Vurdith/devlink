{
  "name": "devlink",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3457",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "public": "npx cloudflared tunnel --url http://localhost:3457",
    "public:dev": "concurrently \"npm run dev\" \"npx cloudflared tunnel --url http://localhost:3457\"",
    "tunnel": "npx cloudflared tunnel --url http://localhost:3457",
    "serveo": "ssh -R 80:localhost:3457 serveo.net",
    "public:serveo": "concurrently \"npm run dev\" \"ssh -R 80:localhost:3457 serveo.net\"",
    "cloudflare": "npx cloudflared tunnel --url http://localhost:3457",
    "public:cf": "concurrently \"npm run dev\" \"npx cloudflared tunnel --url http://localhost:3457\"",
    "kill": "taskkill /f /im node.exe & taskkill /f /im cloudflared.exe",
    "stop": "taskkill /f /im node.exe & taskkill /f /im cloudflared.exe"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.15.0",
    "@types/nodemailer": "^7.0.2",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "emoji-picker-react": "^4.13.3",
    "framer-motion": "^12.23.12",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "openai": "^5.21.0",
    "prisma": "^6.15.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
